{
  "servers": {
    "awesome-ionic-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "awesome-ionic-mcp@latest"]
    },
    "mcp_square_api": {
      "type": "stdio",
      "command": "npx",
      "args": ["square-mcp-server", "start"],
      "env": {
        "ACCESS_TOKEN": "YOUR_SQUARE_SANDBOX_ACCESS_TOKEN_HERE",
        "SANDBOX": "true"
      }
    }
  }
}
