{
  "servers": {
    "awesome-ionic-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "awesome-ionic-mcp@latest"]
    },
    "mcp_square_api": {
      "type": "stdio",
      "command": "npx",
      "args": ["square-mcp-server", "start"],
      "env": {
        "ACCESS_TOKEN": "${SQUARE_ACCESS_TOKEN}",
        "SANDBOX": "${SQUARE_SANDBOX:-true}"
      }
    }
  }
}
